<!-- Probleme avec le css -->
{% block footer %}
    <!-- Footer -->
    <footer class="page-footer font-small mdb-color lighten-3">
        <!-- Footer Elements -->
        <div class="container">
            <!-- Grid row-->
            <div class="row">
                <!-- Grid column -->
                <div class="col-4 align-self-center">
                    <div class="view overlay z-depth-1-half">
                        <!--Université Angers-->
                        <img src="{{ asset('images/logo_angers.jpg') }}" alt="logo_angers" class="img-fluid" width="100px"/>
                    </div>
                </div>
                <div class="col-4 align-self-center">
                    <div class="view overlay z-depth-1-half">
                        <!--Emerald-->
                        <img src="{{ asset('images/emerald.jpg') }}" alt="logo_emerald" class="img-fluid" width="70px"/>
                        <div class="mask rgba-white-light"></div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Copyright -->
        <div class="footer-copyright text-center py-3">
            <p>© 2020 Copyright - <a href="{{ path('contact') }}">Contact</a></p>
        </div>
    </footer>
{% endblock %}